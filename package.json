{
  "name": "catalog",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.1",
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.2",
    "@popperjs/core": "^2.11.0",
    "animate.css": "^4.1.1",
    "axios": "^1.3.2",
    "bootstrap": "^5.3.1",
    "chart.js": "^4.3.3",
    "jsbarcode": "^3.11.5",
    "nouislider": "^15.7.0",
    "qrcode": "^1.5.3"
  },
  "scripts": {
    "parcel:clobber": "rm -fr ./app/assets/builds/*",
    "parcel:watch": "PARCEL_COMMAND=watch yarn parcel:exec --no-hmr",
    "parcel:build": "PARCEL_COMMAND=build yarn parcel:exec --no-cache",
    "parcel:exec": "parcel $PARCEL_COMMAND --no-content-hash"
  },
  "targets": {
    "global": {
      "source": [
        "./app/assets/src/application.js"
      ],
      "distDir": "./app/assets/builds",
      "context": "browser",
      "outputFormat": "global"
    }
  },
  "browserslist": "> 0.35%, not dead",
  "devDependencies": {
    "@parcel/transformer-sass": "=2.12.0",
    "autoprefixer": "^10.4.15",
    "buffer": "^5.5.0||^6.0.0",
    "eslint": "^9.0.0",
    "parcel": "=2.12.0",
    "postcss": "^8.4.31",
    "process": "^0.11.10"
  }
}
